{
  "name": "boilerplate-nodejs-express-api",
  "version": "1.0.0",
  "private": true,
  "description": "boilerplate criado com as configurações básicas de uma API usando nodejs com express escrito com ecmascript 2017",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node -r dotenv/config server.js dotenv_config_path=config/env/.env",
    "debug": "DEBUG=express:* nodemon",
    "windows-debug": "set DEBUG=express:* & nodemon",
    "mount": "sh scripts/mount.sh",
    "passwordGenerate": "node -r dotenv/config scripts/passwordGenerate.js dotenv_config_path=config/env/.env",
    "appSecretGenerate": "node -r dotenv/config scripts/appSecretGenerate.js dotenv_config_path=config/env/.env"
  },
  "keywords": [],
